{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "minItems": 1,
    "items": { 
        "type": "object",
        "required" : [
            "course_number",
            "lecture",
            "section"
        ],
        "properties": {
            "course_number": {
                "type": "string",
                "minLength": 5,
                "maxLength": 5
            },
            "lecture": {
                "type": "string"
            },
            "section": {
                "type": "string"
            }
        }
    } 
}