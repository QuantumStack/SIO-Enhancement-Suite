{
  "name": "SIOrigin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Making Carnegie Mellon SIO less buggy (no pun intended)",
  "homepage_url": "https://quantumstack.xyz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "cookies",
    "https://s3.andrew.cmu.edu/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://s3.andrew.cmu.edu/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://s3.andrew.cmu.edu/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
